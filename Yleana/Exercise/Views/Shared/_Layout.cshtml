<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - Customers</title>
    @Styles.Render("~/Content/Site.css")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/knockout")
    @Scripts.Render("~/bundles/knockoutmapping")
    @Scripts.Render("~/bundles/knockoutvalidation")
    @RenderSection("scripts", required: false)
</head>
<body>
    <header>
        <div>
            <h1>@Html.ActionLink("Yleana Exercise", "Index", "Home")</h1>
        </div>
    </header>
    <div id="content">
        @RenderBody()
        <footer>
            <p>&copy; @DateTime.Now.Year - Yleana</p>
        </footer>
    </div>
</body>
</html>
